<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodleNumericId.moodleDBclass.Course_LMS"
        table="course">

        <id
            name="id"
            column="id"
            type="long">
        </id>

        <property
            name="fullname"
            column="fullname"
            type="string"/>  

        <property
            name="shortname"
            column="shortname"
            type="string"/> 
            
        <property
            name="summary"
            column="summary"
            type="string"/>
            
      <property
            name="startdate"
            column="startdate"
            type="long"/> 
                       
        <property
            name="timecreated"
            column="timecreated"
            type="long"/>
            
        <property
            name="timemodified"
            column="timemodified"
            type="long"/>
            
        <property
            name="enrolstartdate"
            column="enrolstartdate"
            type="long"/>

        <property
            name="enrolenddate"
            column="enrolenddate"
            type="long"/>
            
                    <property
            name="enrolperiod"
            column="enrolperiod"
            type="long"/>
    
    </class>

</hibernate-mapping>