<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class
		name="de.lemo.dms.connectors.moodle_2_3.moodleDBclass.Forum_discussions_LMS"
		table="mdl_forum_discussions">

		<id name="id" column="id" type="long">
		</id>

		<property name="forum" column="forum" type="long" />

		<property name="userid" column="userid" type="string" />

		<property name="name" column="name" type="string" />

		<property name="firstpost" column="firstpost" type="long" />

		<property name="timemodified" column="timemodified" type="long" />

		<property name="usermodified" column="usermodified" type="string" />

		<property name="timestart" column="timestart" type="long" />

		<property name="timeend" column="timeend" type="long" />

	</class>

</hibernate-mapping>
