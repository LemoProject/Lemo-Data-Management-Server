<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodle_2_3.moodleDBclass.Quiz_attempts_LMS"
        table="quiz_attempts">

        <id
            name="id"
            column="id"
            type="long">
            <generator class="native"/>
        </id>

        <property
            name="uniqueid"
            column="uniqueid"
            type="long"/>
            
        <property
            name="sumgrades"
            column="sumgrades"
            type="double"/>
            
        <property
            name="attempt"
            column="attempt"
            type="long"/>

        <property
            name="userid"
            column="userid"
            type="string"/>
            
        <property
            name="quiz"
            column="quiz"
            type="long"/>
            
        <property
            name="timestart"
            column="timestart"
            type="long"/>
   
           <property
            name="timefinish"
            column="timefinish"
            type="long"/>
            
        <property
            name="timemodified"
            column="timemodified"
            type="long"/>
    
    </class>

</hibernate-mapping>
