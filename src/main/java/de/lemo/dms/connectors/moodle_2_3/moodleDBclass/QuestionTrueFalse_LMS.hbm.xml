<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodle_2_3.moodleDBclass.QuestionTrueFalse_LMS"
        table="mdl_question_truefalse">

        <id
            name="id"
            column="id"
            type="long">
                  </id>
  
        <property
            name="question"
            column="question"
            type="long"/>
            
        <property
            name="questiontext"
            column="questiontext"
            type="string"/>

        <property
            name="trueanswer"
            column="trueanswer"
            type="long"/>
            
        <property
            name="falseanswer"
            column="falseanswer"
            type="long"/>
  
    </class>

</hibernate-mapping>
