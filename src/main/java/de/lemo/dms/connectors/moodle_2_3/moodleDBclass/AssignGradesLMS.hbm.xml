<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.lemo.dms.connectors.moodle_2_3.moodleDBclass.AssignGradesLMS"
		table="mdl_assign_grades">

		<id name="id" column="id" type="long">
		</id>

		<property name="assignment" column="assignment" type="long" />

		<property name="user" column="userid" type="long" />

		<property name="grade" column="grade" type="double" />
		
		<property name="timecreated" column="timecreated" type="long" />

		<property name="timemodified" column="timemodified" type="long" />

	</class>

</hibernate-mapping>