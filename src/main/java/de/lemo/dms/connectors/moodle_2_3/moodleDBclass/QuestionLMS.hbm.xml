<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.lemo.dms.connectors.moodle_2_3.moodleDBclass.QuestionLMS"
		table="mdl_question">

		<id name="id" column="id" type="long">
		</id>

		<property name="category" column="category" type="long" />

		<property name="name" column="name" type="string" />

		<property name="questiontext" column="questiontext" type="string" />

		<property name="qtype" column="qtype" type="string" />

		<property name="timecreated" column="timecreated" type="long" />

		<property name="timemodified" column="timemodified" type="long" />
	</class>

</hibernate-mapping>
