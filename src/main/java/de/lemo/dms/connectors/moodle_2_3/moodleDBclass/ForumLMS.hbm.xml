<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.lemo.dms.connectors.moodle_2_3.moodleDBclass.ForumLMS"
		table="mdl_forum">

		<id name="id" column="id" type="long">
		</id>

		<property name="course" column="course" type="long" />

		<property name="name" column="name" type="string" />

		<property name="intro" column="intro" type="string" />

		<property name="assesstimestart" column="assesstimestart"
			type="long" />

		<property name="assesstimefinish" column="assesstimefinish"
			type="long" />

		<property name="timemodified" column="timemodified" type="long" />
	</class>

</hibernate-mapping>
