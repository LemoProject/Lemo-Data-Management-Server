<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.lemo.dms.connectors.moodle_2_3.moodleDBclass.ChatLogLMS"
		table="mdl_chat_messages">

		<id name="id" column="id" type="long">
		</id>

		<property name="chat" column="chatid" type="long" />

		<property name="user" column="userid" type="string" />

		<property name="message" column="message" type="string" />

		<property name="timestamp" column="timestamp" type="long" />

	</class>

</hibernate-mapping>
