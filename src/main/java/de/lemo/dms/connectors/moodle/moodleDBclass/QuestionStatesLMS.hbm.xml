<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodle.moodleDBclass.QuestionStatesLMS"
        table="question_states">

        <id
            name="id"
            column="id"
            type="long">
        </id>

        <property
            name="attempt"
            column="attempt"
            type="long"/>
            
        <property
            name="question"
            column="question"
            type="long"/>
  
        <property
            name="answer"
            column="answer"
            type="string"/>  

        <property
            name="event"
            column="event"
            type="short"/>
            
      	<property
            name="penalty"
            column="penalty"
            type="double"/>
            
        <property
            name="raw_grade"
            column="raw_grade"
            type="double"/>
             
        <property
            name="grade"
            column="grade"
            type="double"/>
  
		<property
            name="timestamp"
            column="timestamp"
            type="long"/>
    
    </class>

</hibernate-mapping>
