<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodleNumericId.moodleDBclass.Quiz_grades_LMS"
        table="quiz_grades">

        <id
            name="id"
            column="id"
            type="long">
            <generator class="native"/>
        </id>
            
        <property
            name="userid"
            column="userid"
            type="string"/>
            
        <property
            name="quiz"
            column="quiz"
            type="long"/> 
            
        <property
            name="grade"
            column="grade"
            type="double"/> 
            
        <property
            name="timemodified"
            column="timemodified"
            type="long"/>
      
    </class>

</hibernate-mapping>
