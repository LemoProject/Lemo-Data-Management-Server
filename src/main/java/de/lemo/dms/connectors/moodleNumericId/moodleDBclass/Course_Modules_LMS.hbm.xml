<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodleNumericId.moodleDBclass.Course_Modules_LMS"
        table="mdl_course_modules">

        <id
            name="id"
            column="id"
            type="long">
        </id>

        <property
            name="module"
            column="module"
            type="long"/>

        <property
            name="instance"
            column="instance"
            type="long"/>
            
        <property
            name="course"
            column="course"
            type="long"/>
            
    </class>

</hibernate-mapping>