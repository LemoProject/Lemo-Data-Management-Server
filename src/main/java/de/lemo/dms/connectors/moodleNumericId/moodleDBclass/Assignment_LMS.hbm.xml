<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class
		name="de.lemo.dms.connectors.moodleNumericId.moodleDBclass.Assignment_LMS"
		table="mdl_assignment">

		<id name="id" column="id" type="long">
		</id>

		<property name="course" column="course" type="long" />

		<property name="name" column="name" type="string" />

		<property name="description" column="description" type="text" />

		<property name="assignmenttype" column="assignmenttype" type="string" />

		<property name="timemodified" column="timemodified" type="long" />

		<property name="timeavailable" column="timeavailable" type="long" />

		<property name="timedue" column="timedue" type="long" />

	</class>

</hibernate-mapping>