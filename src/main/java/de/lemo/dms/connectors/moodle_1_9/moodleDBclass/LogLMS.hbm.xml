<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.lemo.dms.connectors.moodle_1_9.moodleDBclass.LogLMS"
		table="mdl_log">

		<id name="id" column="id" type="long">
			<generator class="native" />
		</id>

		<property name="time" column="time" type="long" />

		<property name="userid" column="userid" type="long" />

		<property name="course" column="course" type="long" />

		<property name="cmid" column="cmid" type="long" />

		<property name="module" column="module" type="string" />

		<property name="action" column="action" type="string" />

		<property name="info" column="info" type="string" />

	</class>

</hibernate-mapping>
