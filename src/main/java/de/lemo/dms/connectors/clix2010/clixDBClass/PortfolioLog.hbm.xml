<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.clix2010.clixDBClass.PortfolioLog"
        table="PORTFOLIO_LOG">

        <id
            name="id"
            column="PORTFOLIO_LOG_ID"
            type="long">
            <generator class="native"/>
        </id>

        <property
            name="lastUpdated"
            column="LASTUPDATED"
            type="string"/>

        <property
            name="component"
            column="COMPONENT_ID"
            type="long"/>

        <property
            name="person"
            column="PERSON_ID"
            type="long"/>

            
        <property
            name="typeOfModification"
            column="TYPE_OF_MODIFICATION"
            type="long"/>         
            
        <property
            name="lastUpdater"
            column="LASTUPDATER_ID"
            type="long"/>
   
            
    </class>

</hibernate-mapping>