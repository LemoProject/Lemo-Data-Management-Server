<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class
		name="de.lemo.dms.connectors.clix2010.clixDBClass.TQtiTestPlayer"
		table="T_QTI_TESTPLAYER">


		<composite-id name="id"
			class="de.lemo.dms.connectors.clix2010.clixDBClass.TQtiTestPlayerPK">

			<key-property name="candidate" column="CANDIDATE_ID" type="long" />
			<key-property name="container" column="CONTAINER_ID" type="long" />
			<key-property name="content" column="CONTENT_ID" type="long" />

		</composite-id>

		<property name="candidate" insert="false" update="false" column="CANDIDATE_ID"
			type="long" />
			
		<property name="container" insert="false" update="false" column="CONTAINER_ID"
			type="long" />

		<property name="content" insert="false" update="false" column="CONTENT_ID"
			type="long" />
			
		<property name="maxScore" column="MAX_SCORE" type="double" />
		
		<property name="started" column="STARTED" type="string" />
		
		<property name="saved" column="SAVED" type="string" />
		
		<property name="evaluated" column="EVALUATED" type="string" />
		
		<property name="created" column="CREATED" type="string" />
		
		<property name="evaluatedScore" column="EVALUATED_SCORE" type="double" />
		
		<property name="resultStatus" column="RESULT_STATUS" type="long" />
		
		<property name="runtimeStatus" column="RUNTIME_STATUS" type="long" />
		
		<property name="remainingDuration" column="REMAINING_DURATION" type="long" />

	</class>

</hibernate-mapping>