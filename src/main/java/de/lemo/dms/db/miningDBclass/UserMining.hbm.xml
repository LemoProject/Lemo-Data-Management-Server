<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class
        name="de.lemo.dms.db.miningDBclass.UserMining"
        table="user">

        <id
            name="id"
            column="id"
            type="long">
        </id>

        <property
            name="lastlogin"
            column="lastlogin"
            type="long"/>
            
        <property
            name="firstaccess"
            column="firstaccess"
            type="long"/>
            
        <property
            name="lastaccess"
            column="lastaccess"
            type="long"/>
                          
        <property
            name="currentlogin"
            column="currentlogin"
            type="long"/>
            
  	<set name="course_user" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseUserMining"/>
		</set>
		
        <set name="group_user" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.GroupUserMining"/>
		</set>
		
        <set name="forum_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.ForumLogMining"/>
		</set>				           

        <set name="wiki_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.WikiLogMining"/>
		</set>

        <set name="course_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseLogMining"/>
		</set>
		
        <set name="quiz_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.QuizLogMining"/>
		</set>		

        <set name="quiz_user" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.QuizUserMining"/>
		</set>

		<set name="question_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.QuestionLogMining"/>
		</set>

		<set name="resource_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.ResourceLogMining"/>
		</set>
		
         <set name="assignment_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.AssignmentLogMining"/>
		</set>
		
         <set name="scorm_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.ScormLogMining"/>
		</set>


       
		 
    </class>

</hibernate-mapping>