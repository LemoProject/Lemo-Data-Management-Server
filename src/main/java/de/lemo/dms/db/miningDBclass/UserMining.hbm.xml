<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.lemo.dms.db.miningDBclass.UserMining" table="user">

		<id name="id" column="id" type="long">
		</id>

		<property name="platform" column="platform" type="long" />

		<property name="login" column="login" type="string" />

		<property name="gender" column="gender" type="boolean" />

		<property name="lastLogin" column="lastlogin" type="long" />

		<property name="firstAccess" column="firstaccess" type="long" />

		<property name="lastAccess" column="lastaccess" type="long" />

		<property name="currentLogin" column="currentlogin" type="long" />

		<set name="courseUsers" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseUserMining" />
		</set>

		<set name="groupUsers" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.GroupUserMining" />
		</set>

		<set name="forumLogs" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.ForumLogMining" />
		</set>

		<set name="wikiLogs" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.WikiLogMining" />
		</set>

		<set name="courseLogs" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseLogMining" />
		</set>

		<set name="quizLogs" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.QuizLogMining" />
		</set>

		<set name="quizUsers" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.QuizUserMining" />
		</set>

		<set name="questionLogs" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.QuestionLogMining" />
		</set>

		<set name="resourceLogs" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.ResourceLogMining" />
		</set>

		<set name="assignmentLogs" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.AssignmentLogMining" />
		</set>

		<set name="scormLogs" inverse="true" lazy="false">
			<key column="user_id" />
			<one-to-many class="de.lemo.dms.db.miningDBclass.ScormLogMining" />
		</set>




	</class>

</hibernate-mapping>