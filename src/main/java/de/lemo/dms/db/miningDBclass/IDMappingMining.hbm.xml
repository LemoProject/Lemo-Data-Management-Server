<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class
        name="de.lemo.dms.db.miningDBclass.IDMappingMining"
        table="id_mapping">

        <id
            name="id"
            column="id"
            type="long">
        </id>
 
        <property
            name="platform"
            column="platform"
            type="long"/>  
                         
        <property
            name="hash"
            column="hash"
            type="text"/>
    </class>

</hibernate-mapping>