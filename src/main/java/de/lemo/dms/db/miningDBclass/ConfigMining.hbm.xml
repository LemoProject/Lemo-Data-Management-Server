<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class
        name="de.lemo.dms.db.miningDBclass.ConfigMining"
        table="config">

        <id
            name="lastModified"
            column="lastmodified"
            type="timestamp">
        </id>

        <property
            name="extractCycle"
            column="extractcycle"
            type="long"/>
              
        <property
            name="elapsedTime"
            column="elapsed_time"
            type="long"/>   
            
        <property
            name="databaseModel"
            column="database_model"
            type="string"/>   
 
        <property
            name="platform"
            column="platform"
            type="long"/>         
                
    </class>

</hibernate-mapping>