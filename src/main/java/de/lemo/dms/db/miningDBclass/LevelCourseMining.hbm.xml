<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.lemo.dms.db.miningDBclass.LevelCourseMining"
		table="level_course">

		<id name="id" column="id" type="long">
		</id>

		<property name="platform" column="platform" type="long" />


		<many-to-one name="course"
			class="de.lemo.dms.db.miningDBclass.CourseMining" cascade="save-update"
			column="course_id" />

		<many-to-one name="level"
			class="de.lemo.dms.db.miningDBclass.LevelMining" cascade="save-update"
			column="level_id" />


	</class>

</hibernate-mapping>