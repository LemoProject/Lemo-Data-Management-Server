<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodle.moodleDBclass.Quiz_question_instances_LMS"
        table="quiz_question_instances">

        <id
            name="id"
            column="id"
            type="long">
            <generator class="native"/>
        </id>

        <property
            name="quiz"
            column="quiz"
            type="long"/>
            
        <property
            name="question"
            column="question"
            type="long"/>

    </class>

</hibernate-mapping>
