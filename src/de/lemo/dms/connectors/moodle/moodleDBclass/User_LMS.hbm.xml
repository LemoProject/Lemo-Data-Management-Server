<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodle.moodleDBclass.User_LMS"
        table="user">

        <id
            name="id"
            column="id"
            type="string">
            <generator class="native"/>
        </id>

        <property
            name="firstaccess"
            column="firstaccess"
            type="long"/>
            
        <property
            name="lastaccess"
            column="lastaccess"
            type="long"/>
            
        <property
            name="lastlogin"
            column="lastlogin"
            type="long"/>
  
        <property
            name="currentlogin"
            column="currentlogin"
            type="long"/>
            
            <property
            name="timemodified"
            column="timemodified"
            type="long"/>
    
    </class>

</hibernate-mapping>
