<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodle.moodleDBclass.Groups_LMS"
        table="groups">

        <id
            name="id"
            column="id"
            type="long">
            <generator class="native"/>
        </id>
           
        <property
            name="courseid"
            column="courseid"
            type="long"/>
  
        <property
            name="name"
            column="name"
            type="string"/>  

        <property
            name="timecreated"
            column="timecreated"
            type="long"/>
            
        <property
            name="timemodified"
            column="timemodified"
            type="long"/>
    
    </class>

</hibernate-mapping>
