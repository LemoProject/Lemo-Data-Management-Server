<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.clix2010.clixDBClass.EComposing"
        table="lemo_db.E_COMPOSING">

        <id
            name="id"
            column="ID"
            type="long">
            <generator class="native"/>
        </id>

        <property
            name="component"
            column="COMPONENT_ID"
            type="long"/>

        <property
            name="composing"
            column="COMPOSING_ID"
            type="long"/>
            
    </class>

</hibernate-mapping>