<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodleNumericId.moodleDBclass.Quiz_LMS"
        table="quiz">

        <id
            name="id"
            column="id"
            type="long">
        </id>

        <property
            name="course"
            column="course"
            type="long"/>

        <property
            name="name"
            column="name"
            type="string"/>
            
        <property
            name="intro"
            column="intro"
            type="string"/>
            
        <property
            name="questions"
            column="questions"
            type="string"/>

        <property
            name="timeopen"
            column="timeopen"
            type="long"/>
            
        <property
            name="timeclose"
            column="timeclose"
            type="long"/>
  
		<property
            name="sumgrade"
            column="sumgrades"
            type="long"/> 
            
		<property
            name="grade"
            column="grade"
            type="long"/> 
            
        <property
            name="timecreated"
            column="timecreated"
            type="long"/>
               
        <property
            name="timemodified"
            column="timemodified"
            type="long"/> 
    
    </class>

</hibernate-mapping>
