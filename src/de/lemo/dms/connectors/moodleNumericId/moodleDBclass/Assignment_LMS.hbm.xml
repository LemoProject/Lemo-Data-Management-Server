<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodleNumericId.moodleDBclass.Assignment_LMS"
        table="assignment">

        <id
            name="id"
            column="id"
            type="long">
            <generator class="native"/>
        </id>

        <property
            name="course"
            column="course"
            type="long"/>

        <property
            name="name"
            column="name"
            type="string"/>

        <property
            name="assignmenttype"
            column="assignmenttype"
            type="string"/>
            
        <property
            name="timemodified"
            column="timemodified"
            type="long"/>
            
        <property
            name="timeavailable"
            column="timeavailable"
            type="long"/>                        

        <property
            name="timedue"
            column="timedue"
            type="long"/> 
            
    </class>

</hibernate-mapping>