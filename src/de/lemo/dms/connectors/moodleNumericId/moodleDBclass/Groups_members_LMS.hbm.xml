<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class
        name="de.lemo.dms.connectors.moodleNumericId.moodleDBclass.Groups_members_LMS"
        table="groups_members">

        <id
            name="id"
            column="id"
            type="long">
            <generator class="native"/>
        </id>

        <property
            name="groupid"
            column="groupid"
            type="long"/>
            
        <property
            name="userid"
            column="userid"
            type="string"/>
            
        <property
            name="timeadded"
            column="timeadded"
            type="long"/>
    
    </class>

</hibernate-mapping>
