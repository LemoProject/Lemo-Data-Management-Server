<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class
        name="de.lemo.dms.db.miningDBclass.CourseMining"
        table="course">

        <id
            name="id"
            column="id"
            type="long">
        </id>

        <property
            name="startdate"
            column="startdate"
            type="long"/>
            
        <property
            name="enrolstart"
            column="enrolstart"
            type="long"/>
            
        <property
            name="enrolend"
            column="enrolend"
            type="long"/>
            
        <property
            name="timecreated"
            column="timecreated"
            type="long"/>
               
        <property
            name="timemodified"
            column="timemodified"
            type="long"/>

        <property
            name="title"
            column="title"
            type="string"/>
            
        <property
            name="shortname"
            column="shortname"
            type="string"/>

		<set name="course_group" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseGroupMining"/>
		</set> 

		<set name="course_forum" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseForumMining"/>
		</set> 
		
		<set name="course_wiki" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseWikiMining"/>
		</set> 

		<set name="course_user" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseUserMining"/>
		</set> 

		<set name="course_quiz" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseQuizMining"/>
		</set> 
		
		<set name="course_assignment" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseAssignmentMining"/>
		</set> 
		
		<set name="course_resource" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseResourceMining"/>
		</set> 

		<set name="wiki_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.WikiLogMining"/>
		</set> 

		<set name="course_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseLogMining"/>
		</set> 

		<set name="quiz_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.QuizLogMining"/>
		</set>
		
		<set name="question_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.QuestionLogMining"/>
		</set>
		
		<set name="quiz_user" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.QuizUserMining"/>
		</set>
		
		<set name="resource_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.ResourceLogMining"/>
		</set>

		<set name="course_scorm" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.CourseScormMining"/>
		</set> 
		
		<set name="degree_course" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.DegreeCourseMining"/>
		</set> 
		
		<set name="scorm_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.ScormLogMining"/>
		</set>

		<set name="assignment_log" inverse="true" lazy="false">
			<key column="id"/>
			<one-to-many class="de.lemo.dms.db.miningDBclass.AssignmentLogMining"/>
		</set>
		                
    </class>

</hibernate-mapping>